# Copy to .env and fill real secrets (DO NOT commit .env)
DATABASE_URL=postgresql://<user>:<password>@<host>:5432/<db>?sslmode=require
JWT_SECRET_KEY=<change-me>
JWT_REFRESH_SECRET_KEY=<change-me>
GOOGLE_CLIENT_ID=<your-google-client-id>
GOOGLE_CLIENT_SECRET=<your-google-client-secret>
GOOGLE_REDIRECT_URI=https://your-domain.com/api/auth/oauth/google/callback
AZURE_STORAGE_ACCOUNT_NAME=<your-storage-account>
AZURE_STORAGE_ACCOUNT_KEY=<your-storage-key>
CORS_ORIGINS=http://your-frontend-domain:3000
NEXT_PUBLIC_API_URL=http://your-backend-domain:8000
